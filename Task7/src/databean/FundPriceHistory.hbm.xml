<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="databean.FundPriceHistory" table="FundPriceHistory">


		<composite-id name="comp_id">
			<key-property name="fund_id" column="fund_id" />
			<key-property name="price_date" type="timestamp" column="price_date" />
		</composite-id>

		<property name="price">
			<column name="price" />
		</property>

        
    </class>
</hibernate-mapping>
